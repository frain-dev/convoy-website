{
	"redirects": [
		{
			"source": "/docs/docs/:match*",
			"destination": "/docs/:match*",
			"permanent": true
		}
	],
	"rewrites": [
		{
			"source": "/docs",
			"destination": "https://convoy.mintlify.dev/docs"
		},
		{
			"source": "/docs/:match*",
			"destination": "https://convoy.mintlify.dev/docs/:match*"
		}
	]
}
